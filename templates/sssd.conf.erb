[sssd]
config_file_version = 2
services = nss, pam
#debug_level = 0xFFF0
domains = LDAP

#[nss]
#filter_users = root
#filter_groups = root

[domain/LDAP]

enumerate = true

id_provider     = ldap
chpass_provider = ldap
auth_provider   = ldap

ldap_uri               = <%= @ldap_uri %>
ldap_user_search_base  = <%= @ldap_searchbase %>
ldap_default_bind_dn   = <%= @ldap_authdn %>
ldap_default_authtok   = <%= @ldap_authpw %>

